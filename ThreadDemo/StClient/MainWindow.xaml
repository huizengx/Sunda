<Window x:Class="StClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel>
            <Button x:Name="Btnconnect" Click="Btnconnect_Click">connect</Button>
            <!--<Button x:Name="TaskBtn" Click="TaskBtn_Click">Task</Button>
            <Button x:Name="Async_AwaitBtn" Click="Async_AwaitBtn_Click">Async_Await</Button>
            <Button x:Name="Thread_contend" Click="Thread_contend_Click">Thread_contend</Button>
            <Button x:Name="DeadLock" Click="DeadLock_Click">DeadLock</Button>-->
        </StackPanel>
        <StackPanel Grid.Column="1">
            <Button x:Name="ClearLogBtn" Click="ClearLogBtn_Click">Send</Button>
        </StackPanel>

    </Grid>
</Window>
